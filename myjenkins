pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building is running'
                bat'hello world'
            }
        }
         stage('Test') {
            steps {
                echo 'testing is running'
            }
        }
         stage('deploy') {
            steps {
                echo 'deploying is running'
                bat'heloo my friend'
            }
        }
    }
}
